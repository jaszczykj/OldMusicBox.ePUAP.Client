@model OldMusicBox.ePUAP.Demo.Models.Home.SignedModel
@{
    ViewBag.Title = "Signed";
}

<h2>Signed in with ePUAP</h2>

<h4>User Claims</h4>
<table style="border-collapse : collapse" border="1" cellpadding="3">

    <tr>
        <th>
            Claim Type
        </th>
        <th>
            Claim Value
        </th>
    </tr>
    @foreach (var claim in Model.Claims)
    {
        <tr>
            <td>
                @claim.Type
            </td>
            <td>
                @claim.Value
            </td>
        </tr>
    }

</table>